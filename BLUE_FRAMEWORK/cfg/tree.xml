<!DOCTYPE root SYSTEM 'dtd/tree.dtd'>
<root options="1">
    <lib on="1">user</lib>
    <mod on="1">benchmark</mod>

    <css>bootstrap_dark</css>
    <css>elusive-webfont</css>
    <css>elusive-webfont-ie7</css>
    <css>base</css>

    <js>jQuery-2</js>
    <js>bootstrap</js>
    <js>bootstrap_dropdown_hover</js>
    <js>bootstrap_extend</js>

    <page id="index" name="Allegro generator" layout="index" options="1111" changefreq="always" priority="0.8">
        <sub id="render" layout="index" name="Szablon" options="11110">
            <lib on="1">user</lib>
            <mod on="1">benchmark</mod>
            <mod on="1" block="main_content">login</mod>
            <mod on="1" block="main_content">allegro_generator</mod>
        </sub>

        <sub id="upload" layout="empty" name="Upload" options="11111">
            <lib on="1">user</lib>
            <mod on="1">benchmark</mod>
            <mod on="1" block="main_content">allegro_generator</mod>
        </sub>

        <mod on="1" block="main_content">login</mod>
        <mod on="1" block="main_content">allegro_generator</mod>

        <css>dropzone</css>
        <js>dropzone</js>
    </page>

    <page id="error404" name="Strona nie znaleziona" layout="404" options="1001">
        <mod on="1" param="breadcrumbs">main</mod>
        <css>styles</css>
    </page>
</root>